[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[core]
	excludesfile = /Users/svz/.gitignore_global
	editor = cursor --wait
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[commit]
	template = /Users/svz/.stCommitMsg
[alias]
    st = status -s
    sta = status
    conf = config --global --edit
    cge = config --global --edit
    ci = commit
    co = checkout
    cod = checkout .
    rh = reset HEAD
    aa = add -A
    cdf = clean -df
    br = branch
    bra = branch -a
    pr = pull --rebase
    amend = commit -a --amend --no-edit
    ciam = commit -a --amend --no-edit
    wtl = worktree list
    wtr = worktree remove
    wta = worktree add
